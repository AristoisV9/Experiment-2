#include<iostream>
 
using namespace std;
 
main()
{
	int a;
	int b;
	int c;
	int d;
	int e;
 
 	a=22;
	c=0;
	d=1;
	cout << "This program will show the next 20 Fibonacci numbers after 0 and 1";
	cin.ignore();
	cout << "Fibonacci numbers:\n";
 
	for (b=0;b<a;b++)
	{
    	if (b<=1)
    		e = b;
    	else
    	{
	        e = c + d;
	        c = d;
	        d = e;
    	}
    	cout << e <<", ";
 	}
 
   return 0;
}
